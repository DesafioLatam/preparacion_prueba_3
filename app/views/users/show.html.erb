<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Duration</th>
      <th>Genres</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_songs.each do |user_song| %>
      <tr>
        <td><%= user_song.song.name %></td>
        <td><%= user_song.position %> </td>
        <td><%= user_song.song.duration %></td>
        <td><%= link_to 'Destroy', [@user, user_song], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
